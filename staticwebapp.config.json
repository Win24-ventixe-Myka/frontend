{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/assets/*",
      "serve": "/assets/"
    },
    {
      "route": "/events/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/dashboard/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}